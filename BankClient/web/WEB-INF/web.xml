<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>loginController</servlet-name>
        <servlet-class>servlet.loginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RutTienController</servlet-name>
        <servlet-class>servlet.RutTienController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>indexController</servlet-name>
        <servlet-class>servlet.indexController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChuyenTienController</servlet-name>
        <servlet-class>servlet.ChuyenTienController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LichSuGiaoDichController</servlet-name>
        <servlet-class>servlet.LichSuGiaoDichController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>XemSoDuController</servlet-name>
        <servlet-class>servlet.XemSoDuController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>index</servlet-name>
        <servlet-class>servlet.index</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>servlet.login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginController</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RutTienController</servlet-name>
        <url-pattern>/RutTien</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>indexController</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChuyenTienController</servlet-name>
        <url-pattern>/ChuyenTien</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LichSuGiaoDichController</servlet-name>
        <url-pattern>/history</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>XemSoDuController</servlet-name>
        <url-pattern>/xemsodu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>index</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
